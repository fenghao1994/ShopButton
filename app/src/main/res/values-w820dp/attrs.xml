<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="TowButton">
        <!--中间edittext属性-->

        <attr name="textBackground" format="reference|color"/>
        <attr name="textSize" format="dimension"/>
        <attr name="textColor" format="color"/>
        <attr name="text" format="string"/>
        <attr name="textCursorVisible" format="boolean"/>

        <!--左边button属性-->
        <attr name="leftBackground" format="reference|color"/>

        <!--右边button属性-->
        <attr name="rightBackground" format="reference|color"/>

        <!-- btn默认宽度-->
        <attr name="btnWidth" format="dimension"/>
        <!--最小数-->
        <attr name="minNumber" format="float"/>
        <!--最大数-->
        <attr name="maxNumber" format="float"/>
    </declare-styleable>

</resources>